{
  "extends": "airbnb",
  "globals": {
    // false indicates the value cannot be overwritten
    "document": false,
    "it": false,
    "window": false
  },
  "rules": {
    "react/sort-comp": 0,
    "object-curly-spacing": 0,
    "react/prop-types": 0,
    "react/jsx-closing-bracket-location": 0,
    "comma-dangle": 0,
    "arrow-parens": 0,
    "indent": 0,
    "class-methods-use-this": 0,
    "quote-props": 0,
    "prefer-template": 0
  }
}
